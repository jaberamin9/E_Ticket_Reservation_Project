<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.blogspot.softlabsja.e_ticketreservationadmin">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ETicketReservationAdmin"
        android:usesCleartextTraffic="true"
        tools:targetApi="n">
        <activity android:name=".BookedSeat.UpdateBookedSeat.UpdateBookedSeatActivity"></activity>
        <activity android:name=".BookedSeat.BookedSeatActivity" />
        <activity android:name=".UserList.AddUpdateUser.AddUpdateUserActivity" />
        <activity android:name=".UserList.UserListActivity" />
        <activity android:name=".BusList.AddUpdateBus.AddUpdateBusActivity" />
        <activity android:name=".BusList.BusListActivity" />
        <activity android:name=".Dashboard.DashboardActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>